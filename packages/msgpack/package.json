{
  "name": "@urlpack/msgpack",
  "version": "1.0.3",
  "license": "MIT",
  "homepage": "https://github.com/daangn/urlpack/tree/main/packages/msgpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/daangn/urlpack.git",
    "directory": "packages/msgpack"
  },
  "source": "./src/index.ts",
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public",
    "main": "./lib/index.mjs"
  },
  "files": [
    "src/**/*.ts",
    "src/*.res",
    "lib/*.mjs",
    "lib/*.cjs",
    "lib/*.d.ts",
    "lib/*.map"
  ],
  "scripts": {
    "prepack": "yarn build",
    "build": "rescript build && nanobundle build --standalone",
    "test": "uvu -r tsm",
    "test:watch": "yarn test || true && watchlist src tests -- yarn test"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "esbuild": "^0.14.9",
    "nanobundle": "^0.0.21",
    "rescript": "^9.1.4",
    "rescript-msgpack": "workspace:^0.1.0",
    "tsm": "^2.2.1",
    "typescript": "^4.5.4",
    "uvu": "^0.5.2",
    "watchlist": "^0.3.1"
  }
}
